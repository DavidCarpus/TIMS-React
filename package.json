{
    "name": "Milton",
    "version": "0.1.0",
    "proxy": "http://localhost:45001",
    "private": true,
    "devDependencies": {
        "node-sass-chokidar": "^0.0.3",
        "nodemon": "^1.11.0",
        "react-scripts": "1.0.7"
    },
    "dependencies": {
        "axios": "^0.16.2",
        "bootstrap": "4.0.0-alpha.6",
        "classnames": "^2.2.5",
        "concurrently": "^3.4.0",
        "date-fns": "^1.28.5",
        "email-validator": "^1.1.1",
        "google-libphonenumber": "^3.0.0",
        "material-ui": "^0.19.1",
        "react": "^15.6.1",
        "react-addons-css-transition-group": "^15.6.0",
        "react-addons-transition-group": "^15.6.0",
        "react-bootstrap": "^0.31.0",
        "react-collapsible": "^2.0.0",
        "react-dom": "^15.6.1",
        "react-dropdown": "^1.2.5",
        "react-dropzone": "^4.1.1",
        "react-fileupload": "^2.4.0",
        "react-redux": "^5.0.5",
        "react-responsive-accordion": "^1.0.0",
        "react-router": "^4.1.1",
        "react-router-bootstrap": "^0.24.2",
        "react-router-dom": "^4.1.1",
        "react-router-redux": "^4.0.8",
        "reactstrap": "^4.8.0",
        "redux": "^3.6.0",
        "redux-form": "^7.0.3",
        "redux-promise-middleware": "^4.3.0",
        "redux-thunk": "^2.2.0"
    },
    "scripts": {
        "eject": "react-scripts eject",
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start": "react-scripts start -- --include-path ./src",
        "build": "react-scripts build",
        "pushProd": "rsync -auv build/ A2:./miltonnh.us/www/ --delete  && rsync -auv server/ A2:./miltonnh.us/server/",
        "test": "react-scripts test --env=jsdom",
        "startServer": "cd server && node app.js",
        "startTestServer": "cd server && node app.js",
        "buildTest": "PUBLIC_URL=\"http://carpusconsulting.com/milton/\" react-scripts build",
        "pushTest": "rsync -auv build/ A2:./test.miltonnh.us/www  --exclude .htaccess --delete  && rsync -auv server/ A2:./test.miltonnh.us/server/",
        "pushDev": "rsync -auv build/ A2:./dev.miltonnh.us/www  --exclude .htaccess --delete  && rsync -auv server/ A2:./dev.miltonnh.us/server/",
        "dev": "concurrently \"yarn start\"  \"cd server && nodemon app.js\" \"npm run watch-css\" "
    }
}
