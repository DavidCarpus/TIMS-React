{
  "scripts": {
    "start": "node app.js",
    "kinit": "knex init",
    "imap": "node imap.js",
    "knex:seed:make": "knex --knexfile libs/db/knexfile.js seed:make",
    "knex:seed:run": " knex  --knexfile libs/db/knexfile.js seed:run",
    "migrate:latest": "knex  --knexfile libs/db/knexfile.js migrate:latest",
    "migrate:rollback": "knex  --knexfile libs/db/knexfile.js migrate:rollback",
    "migrate:make": "knex  --knexfile libs/db/knexfile.js migrate:make",
    "dev": "node imap.js",
    "initDB": "npm run migrate:rollback && npm run migrate:latest && npm run knex:seed:run json",
    "time": "grep \"`date '+%b %d %Y'`\" ../../logDev.txt | cut -d ' ' -f 2,6-7 | cut -d ':' -f 1-2 | sort | uniq",
    "timey": "grep \"`date --date='yesterday ' '+%b %d %Y'`\" ../../logDev.txt | cut -d ' ' -f 2,6-7 | cut -d ':' -f 1-2 | sort | uniq"
  },
